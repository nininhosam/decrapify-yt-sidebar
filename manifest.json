{
  "manifest_version": 2,
  "name": "Decrapify YouTube Sidebar",
  "version": "1.0",
  "description": "Reorders elements in the YouTube sidebar to match my preferred layout.",
  
  "content_scripts": [
    {
      "matches": [
        "*://www.youtube.com/*",
        "*://youtube.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],

  "permissions": []
}
